<div id="<%= dom_id carro %>">
  <p>
    <strong>Marca:</strong>
    <%= carro.marca %>
  </p>

  <p>
    <strong>Modelo:</strong>
    <%= carro.modelo %>
  </p>

  <p>
    <strong>Ano:</strong>
    <%= carro.ano %>
  </p>

  <p>
    <strong>Cor:</strong>
    <%= carro.cor %>
  </p>

  <p>
    <strong>Placa:</strong>
    <%= carro.placa %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= carro.status %>
  </p>

  
  <p>
    <strong>Diaria:</strong>
    <%= carro.diaria %>
  </p>
  
  <p>
    <strong>Categoria:</strong>
    <%= carro.categoria.nome if carro.categoria %>
  </p>
   

</div>
